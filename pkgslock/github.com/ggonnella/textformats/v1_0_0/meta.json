{
  "name": "textformats",
  "version": "1.0.0",
  "author": "Giorgio Gonnella",
  "desc": "Easy specification of text formats for structured data",
  "license": "ISC",
  "skipDirs": [],
  "skipFiles": [],
  "skipExt": [],
  "installDirs": [],
  "installFiles": [],
  "installExt": [
    "nim"
  ],
  "requires": [
    {
      "name": "nim",
      "str": ">= 1.0.2 & <= 1.4.8",
      "ver": {
        "kind": "verIntersect",
        "verILeft": {
          "kind": "verEqLater",
          "ver": "1.0.2"
        },
        "verIRight": {
          "kind": "verEqEarlier",
          "ver": "1.4.8"
        }
      }
    },
    {
      "name": "cligen",
      "str": ">= 1.5.5",
      "ver": {
        "kind": "verEqLater",
        "ver": "1.5.5"
      }
    },
    {
      "name": "yaml",
      "str": ">= 0.14",
      "ver": {
        "kind": "verEqLater",
        "ver": "0.14"
      }
    },
    {
      "name": "regex",
      "str": ">= 0.15",
      "ver": {
        "kind": "verEqLater",
        "ver": "0.15"
      }
    },
    {
      "name": "nimpy",
      "str": "any version",
      "ver": {
        "kind": "verAny"
      }
    }
  ],
  "bin": [
    "tf_spec",
    "tf_validate",
    "tf_decode",
    "tf_encode"
  ],
  "binDir": "cli",
  "srcDir": "src",
  "backend": "c",
  "ref": "refs/tags/v1.0.0"
}