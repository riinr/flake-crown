{
  "name": "py2nim",
  "version": "0.1.3",
  "author": "Leon Lysak",
  "desc": "Py2Nim is a tool to translate Python code to Nim. The output is idiomatic Nim code, meant to be tweaked by hand after the translation process.",
  "license": "MIT",
  "skipDirs": [],
  "skipFiles": [],
  "skipExt": [],
  "installDirs": [],
  "installFiles": [],
  "installExt": [
    "nim",
    "py",
    "nims"
  ],
  "requires": [
    {
      "name": "nim",
      "str": ">= 1.6.0",
      "ver": {
        "kind": "verEqLater",
        "ver": "1.6.0"
      }
    },
    {
      "name": "compiler",
      "str": "#head",
      "ver": {
        "kind": "verSpecial",
        "spe": "#head"
      }
    },
    {
      "name": "nimscripter",
      "str": ">= 1.0.3",
      "ver": {
        "kind": "verEqLater",
        "ver": "1.0.3"
      }
    }
  ],
  "bin": [
    "py2nim"
  ],
  "binDir": "",
  "srcDir": "src",
  "backend": "c",
  "ref": "refs/heads/main"
}