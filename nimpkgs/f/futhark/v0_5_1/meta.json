{
  "name": "futhark",
  "version": "0.5.1",
  "author": "PMunch",
  "desc": "A package which uses libclang to parse C headers into Nim files for easy interop",
  "license": "MIT",
  "skipDirs": [],
  "skipFiles": [],
  "skipExt": [],
  "installDirs": [],
  "installFiles": [],
  "installExt": [
    "nim"
  ],
  "requires": [
    {
      "name": "nim",
      "str": ">= 1.4.8",
      "ver": {
        "kind": "verEqLater",
        "ver": "1.4.8"
      }
    },
    {
      "name": "https://github.com/PMunch/libclang-nim",
      "str": "#head",
      "ver": {
        "kind": "verSpecial",
        "spe": "#head"
      }
    },
    {
      "name": "termstyle",
      "str": "any version",
      "ver": {
        "kind": "verAny"
      }
    },
    {
      "name": "macroutils",
      "str": "any version",
      "ver": {
        "kind": "verAny"
      }
    }
  ],
  "bin": [
    "opir"
  ],
  "binDir": "",
  "srcDir": "src",
  "backend": "c",
  "ref": "refs/tags/v0.5.1"
}